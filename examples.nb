      SuperscriptBox["x", "2"], "+", 
      SuperscriptBox["y", "2"]}], ")"}], " ", 
    SuperscriptBox["\[Omega]", "4"], " ", 
    SubscriptBox["\[Mu]", "0"]}], 
   RowBox[{"32", " ", "c", " ", 
    SuperscriptBox["\[Pi]", "2"], " ", 
    SuperscriptBox[
     RowBox[{"(", 
      RowBox[{
       SuperscriptBox["x", "2"], "+", 
       SuperscriptBox["y", "2"], "+", 
       SuperscriptBox["z", "2"]}], ")"}], "2"]}]]}]], "Input",
 CellChangeTimes->{
  3.796693207758559*^9},ExpressionUUID->"ece1eee1-44e9-41a5-9fc0-\
e02099a1d15c"],

Cell[BoxData[
