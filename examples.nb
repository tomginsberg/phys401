     RowBox[{"x", ",", "y", ",", "z"}], "}"}], "\[Element]", 
    RowBox[{"ImplicitRegion", "[", 
     RowBox[{
      RowBox[{
       RowBox[{
        SuperscriptBox["x", "2"], "+", 
        SuperscriptBox["y", "2"], "+", 
        SuperscriptBox["z", "2"]}], "\[Equal]", 
       SuperscriptBox["r", "2"]}], ",", 
      RowBox[{"{", 
       RowBox[{"x", ",", "y", ",", "z"}], "}"}]}], "]"}]}]], 
  FractionBox[
   RowBox[{
    SubsuperscriptBox["p", "0", "2"], " ", 
